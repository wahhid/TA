<?xml version="1.0" encoding="windows-1252"?>
<openerp>
    <data>
        <!-- IM/CATEGORY-->				
        <record model="ir.ui.view" id="view_im_category_form">
            <field name="name">im.category.form</field>
            <field name="model">im.category</field>						
            <field name="type">form</field>						
            <field name="arch" type="xml">
                <form string="Category" version="7.0">
                	<sheet>
	                    <group>                            
	                        <field name="name"/>
	                    </group>	                		                	
                	</sheet>  
                </form>
            </field>
        </record>
        
		<record model="ir.ui.view" id="view_im_category_tree">
            <field name="name">im.category.tree</field>
            <field name="model">im.category</field>			
            <field name="type">tree</field>						
            <field name="arch" type="xml">
                <tree string="Category">
	            	<field name="name"/>
                </tree>
            </field>
        </record>        

		<!-- IM/PRIORITY-->				
        <record model="ir.ui.view" id="view_im_priority_form">
            <field name="name">im.priority.form</field>
            <field name="model">im.priority</field>						
            <field name="type">form</field>						
            <field name="arch" type="xml">
                <form string="Priority" version="7.0">
                	<sheet>
	                    <group>                            
	                        <field name="name"/>
	                    </group>	                		                	
                	</sheet>  
                </form>
            </field>
        </record>
        
		<record model="ir.ui.view" id="view_im_priority_tree">
            <field name="name">im.priority.tree</field>
            <field name="model">im.priority</field>			
            <field name="type">tree</field>						
            <field name="arch" type="xml">
                <tree string="Priority">
	            	<field name="name"/>
                </tree>
            </field>
        </record>        

        
        <!-- IM/ACTION -->
        
        <!-- CATEGORY -->        
		<record model="ir.actions.act_window" id="action_im_category">
            <field name="name">Category</field>
            <field name="res_model">im.category</field>
            <field name="view_mode">tree,form</field>                        
        </record>

		<!-- PRIORITY -->        
		<record model="ir.actions.act_window" id="action_im_priority">
            <field name="name">Priority</field>
            <field name="res_model">im.priority</field>
            <field name="view_mode">tree,form</field>                        
        </record>
        		
    </data>                
</openerp>